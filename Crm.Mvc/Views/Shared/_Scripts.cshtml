<script>
    $(document).ready(function (){
        $("ul li").removeClass("menu-item-active");
        let url = window.location.pathname;
        let action = url.split('/');
        let par = $('a[href*="'+action[1]+'"]').first().parent();
        par.addClass("active");
        par.parent().parent().addClass("has-sub open");
    });
</script>
